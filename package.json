{
  "name": "report-backend",
  "version": "1.0.0",
  "description": "backend to support word processor frontend",
  "main": "server.js",
  "scripts": {
    "test": "jest --verbose",
    "dev": "ts-node-dev --respawn --pretty --transpile-only src/server.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "jest": "^26.6.3",
    "typescript": "^4.2.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@types/uuid": "^8.3.0",
    "mocha": "^8.3.2",
    "ts-jest": "^26.5.3",
    "ts-loader": "^6.2.2",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "node_modules/variables/.+\\.(j|t)sx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!variables/.*)"
    ]
  }
}
